  "main": "index.js",
  "dependencies": {
    "gitbook-plugin-accordion": "^1.1.3",
    "gitbook-plugin-github": "^2.0.0",
    "gitbook-plugin-prism": "^2.4.0",
    "gitbook-plugin-validate-links": "^0.1.1"
  },
  "devDependencies": {
    "gitbook-cli": "2.3.0",
    "rimraf": "2.5.4"
  },
  "scripts": {
    "docs:clean": "rimraf _book",
    "docs:prepare": "gitbook install",
    "docs:watch": "npm run docs:prepare && gitbook serve",
    "docs:build": "npm run docs:prepare && npm run docs:clean && gitbook build",
    "docs:publish": "npm run docs:build && cd _book && git init && git commit --allow-empty -m 'update book' && git checkout -b gh-pages && git add . && git commit -am 'update book' && git push git@github.com:foxhound87/mobx-react-form.git gh-pages --force"
  },
